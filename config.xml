<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.lisahoroscope" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Horoscope</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/ic_launcher-ldpi.png" />
        <icon density="mdpi" src="resources/android/icon/ic_launcher-mdpi.png" />
        <icon density="hdpi" src="resources/android/icon/ic_launcher-hdpi.png" />
        <icon density="xhdpi" src="resources/android/icon/ic_launcher-xhdpi.png" />
        <icon density="xxhdpi" src="resources/android/icon/ic_launcher-xxhdpi.png" />
        <icon density="xxxhdpi" src="resources/android/icon/ic_launcher-xxxhdpi.png" />
        <splash density="port-mdpi" src="resources/android/splash/background-mdpi.png" />
        <splash density="port-hdpi" src="resources/android/splash/background-hdpi.png" />
        <splash density="port-xhdpi" src="resources/android/splash/background-xhdpi.png" />
        <splash density="port-xxhdpi" src="resources/android/splash/background-xxhdpi.png" />
        <splash density="port-xxxhdpi" src="resources/android/splash/background-xxxhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="resources/ios/icon/Icon-App-57x57@1x.png" width="57" />
        <icon height="114" src="resources/ios/icon/Icon-App-57x57@2x.png" width="114" />
        <icon height="40" src="resources/ios/icon/Icon-App-40x40@1x.png" width="40" />
        <icon height="80" src="resources/ios/icon/Icon-App-40x40@2x.png" width="80" />
        <icon height="120" src="resources/ios/icon/Icon-App-40x40@3x.png" width="120" />
        <icon height="50" src="resources/ios/icon/Icon-Small-50x50@1x.png" width="50" />
        <icon height="100" src="resources/ios/icon/Icon-Small-50x50@2x.png" width="100" />
        <icon height="60" src="resources/ios/icon/Icon-App-60x60@1x.png" width="60" />
        <icon height="120" src="resources/ios/icon/Icon-App-60x60@2x.png" width="120" />
        <icon height="180" src="resources/ios/icon/Icon-App-60x60@3x.png" width="180" />
        <icon height="72" src="resources/ios/icon/Icon-App-72x72@1x.png" width="72" />
        <icon height="144" src="resources/ios/icon/Icon-App-72x72@2x.png" width="144" />
        <icon height="76" src="resources/ios/icon/Icon-App-76x76@1x.png" width="76" />
        <icon height="152" src="resources/ios/icon/Icon-App-76x76@2x.png" width="152" />
        <icon height="167" src="resources/ios/icon/Icon-App-83.5x83.5@2x.png" width="167" />
        <icon height="29" src="resources/ios/icon/Icon-App-29x29@1x.png" width="29" />
        <icon height="58" src="resources/ios/icon/Icon-App-29x29@2x.png" width="58" />
        <icon height="87" src="resources/ios/icon/Icon-App-29x29@3x.png" width="87" />
        <splash height="1136" src="resources/ios/splash/Default-568h@2x.png" width="640" />
        <splash height="1334" src="resources/ios/splash/Default-667h@2x.png" width="750" />
        <splash height="2208" src="resources/ios/splash/Default-Portrait-736h@3x.png" width="1242" />
        <splash height="1242" src="resources/ios/splash/Default-Landscape-736h@3x.png" width="2208" />
        <splash height="1536" src="resources/ios/splash/Default-Landscape@2x.png" width="2048" />
        <splash height="2048" src="resources/ios/splash/Default-Portrait@2x.png" width="1536" />
        <splash height="1024" src="resources/ios/splash/Default-Portrait.png" width="768" />
        <splash height="960" src="resources/ios/splash/Default@2x.png" width="640" />
    </platform>
    <preference name="orientation" value="portrait" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <plugin name="cordova-sqlite-storage" spec="~2.0.0" />
    <icon src="resources/android/icon/ic_launcher-xhdpi.png" />
    <plugin name="cordova-plugin-facebook4" spec="~1.7.4">
        <variable name="APP_ID" value="1333155163379937" />
        <variable name="APP_NAME" value="Lisa Horoscope" />
    </plugin>
    <plugin name="phonegap-plugin-push" spec="~1.9.2">
        <variable name="SENDER_ID" value="397348636938" />
    </plugin>
</widget>
